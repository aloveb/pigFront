<view class="top">
  <view class="rig">
  <image bindtap="note" class="spic" src="../../images/note.jpg" background-size="cover"></image>
  </view>
<view class="addtext">我的订单</view>
  <view class="lef">
    <image bindtap="me" class="spic" src="../../images/me.jpg" background-size="cover"></image>
  </view>
</view>

<!--历史订单-->
<view>
  <form >
  <view wx:for="{{item}}" class="order">
    <view class="orderInfor">
        <view class="orderText">车位地址：{{item.parkArea}}-{{item.parkBuild}}-{{item.parkNum}}</view> 
        <view class="orderText">日期：{{item.orderDate}} ------ 类型：{{item.orderType}}</view>
        <view class="orderText">价格：{{item.price}} 猪猪币 ------ 状态：{{item.orderState}}</view>
    </view>
    <view class="orderButton">
      <button class="button1">
        <image bindtap="rent" class="spic" src="../../images/me.jpg">编辑</image>
      </button>
      <button class="button1">
        <image bindtap="rent" class="spic" src="../../images/me.jpg">删除</image>
      </button>      
    </view>
  </view>
  </form>
</view>